    font-size:1.2em;
    padding: 0;
}

.art-footer .art-widget,
.art-footer .art-widget a,
.art-footer .art-widget a:link,
.art-footer .art-widget a:visited,
.art-footer .art-widget a:hover
{
    color: #A2A8A9;
font-size: 11px;
    text-align: left;
}

.art-footer .art-widget a,
.art-footer .art-widget a:link
{
  color: #5DBB89;
text-decoration: none;
}

.art-footer .art-widget a:visited
{
  color: #888F91;
}

DIV[id^="GMPmap"] IMG 
{
    margin: 0;
}


/*----- Widget Últimas Noticias -----*/
#nav_menu-2 ul li  {
	margin-bottom: 25px;
/*	padding: 3px 3px 3px 35px;*/
	background: url('images/blockcontentbullets.png') no-repeat 0px 0px;
	-moz-text-shadow: 1px 1px 1px #fff;
	-webkit-text-shadow: 1px 1px 1px #fff;
	text-shadow: 1px 1px 1px #fff;
	-webkit-transition: all 0.3s ease-in-out;
	-moz-transition: all 0.3s ease-in-out;
	transition: all 0.3s ease-in-out;
	cursor: pointer;
}

#nav_menu-2 ul li:hover{
	background: #d3d3d3 url('images/blockcontentbullets.png') no-repeat 0px 0px;
	-moz-border-radius: 5px;
	-webkit--moz-border-radius: 5px;
	border-radius: 5px;
	padding-left: 30px;
}

#nav_menu-2 ul li a {
	font-size: 16px;
	font-weight: bold;
	color: #4E802D;
	font-family: Courier;
	
}



